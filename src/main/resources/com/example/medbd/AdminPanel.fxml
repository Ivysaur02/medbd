                </Tab>
                <Tab fx:id="UserTab" text="Пользователи">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="UserTable" layoutX="4.0" layoutY="6.0" prefHeight="328.0" prefWidth="686.0">
