<AnchorPane fx:id="AdmPanel" prefHeight="511.0" prefWidth="735.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.medbd.controllers.AdmController">
